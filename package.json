{
  "name": "task-manager-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit && echo ",
    "prepare": "husky",
    "format": "prettier --check --ignore-path .gitignore node_modules .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "lint:css": "stylelint './app/**/*.module.css'",
    "seed": "sh ./seed.sh"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "drop-tables": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/drop-tables.ts"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.4",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.10.2",
    "@vercel/blob": "^0.22.1",
    "classnames": "^2.5.1",
    "negotiator": "^0.6.3",
    "next": "14.1.1",
    "next-absolute-url": "^1.2.2",
    "prisma": "^5.10.2",
    "ramda": "^0.29.1",
    "react": "^18",
    "react-color": "^2.19.3",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.51.0",
    "react-intl": "^6.6.2",
    "react-modal": "^3.16.1",
    "react-select": "^5.8.0",
    "rgb-hex": "^4.1.0",
    "sortablejs": "^1.15.2",
    "uuid": "^9.0.1",
    "yet-another-react-lightbox": "^3.17.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20",
    "@types/ramda": "^0.29.11",
    "@types/react": "^18",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18",
    "@types/react-modal": "^3.16.3",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.10",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}
